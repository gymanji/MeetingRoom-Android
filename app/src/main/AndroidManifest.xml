<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.airwatch.meetingroom" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <receiver
        android:name=".MyReceiver"
        android:permission="com.airwatch.admin.sdk.BROADCAST" >
        <intent-filter>
            <action android:name="com.myapplication.airwatchsdk.BROADCAST" />
        </intent-filter>
    </receiver>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MeetingRooms"
            android:label="@string/title_activity_meeting_rooms" >
        </activity>

        <receiver
            android:name=".MyReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>

        <service android:name=".AWSDKIntentService">

        </service>
    </application>


</manifest>
